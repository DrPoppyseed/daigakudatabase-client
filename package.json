  "version": "0.2.1",
  "packageManager": "yarn@3.2.0",
  "license": "MIT",
  "description": "â› Active development branch of DaigakuDatabase.com",
  "repository": {
    "url": "https://github.com/DrPoppyseed/daigakudatabase-client",
    "type": "git"
  },
  "scripts": {
    "start": "PORT=3001 react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --watchAll=false",
    "test:watch": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ js,jsx,ts,tsx }'",
    "lint:fix": "eslint --fix 'src/**/*.{ js,jsx,ts,tsx }'",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "format:check": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">= 16.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn format"
    ]
  },
